{
  "name": "test-case-document-generator",
  "version": "1.0.0",
  "description": "React app that converts JSON test cases to PDF and Excel documents with random images for each step",
  "main": "server/index.js",
  "scripts": {
    "start": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server/index.js",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "install-all": "npm install && cd client && npm install"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "cors": "^2.8.5",
    "exceljs": "^4.4.0",
    "express": "^4.18.2",
    "fs-extra": "^11.1.1",
    "jspdf": "^2.5.1",
    "multer": "^1.4.5-lts.1",
    "path": "^0.12.7",
    "puppeteer": "^21.11.0",
    "sharp": "^0.32.6"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.1"
  },
  "keywords": [
    "react",
    "pdf",
    "excel",
    "test-cases",
    "json",
    "images"
  ],
  "author": "",
  "license": "MIT"
}
